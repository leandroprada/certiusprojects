<?xml version="1.0" encoding="ISO-8859-1" ?>
<grammar xml:lang="en-US" version="1.0" root="_ContractOrMember" tag-format="swi-semantics/1.0">
<!--
   collect contract number or member number
-->

<meta name="swirec_max_speech_duration" content="14000"/>
<meta name="swirec_compile_parser" content="1"/>
<meta name="swirec_simple_result_key" content="MEANING"/>
<meta name="swirec_optimization" content="7"/> <!-- This makes it so the xml compilation level is the same as with sgc -->

  <rule id="_ContractOrMember" scope="public">
  <one-of>
  <item>
    <ruleref uri="#CONTRACT_NUM_ALPHA_FIRST_TWO" tag="
       SWI_meaning = CONTRACT_NUM_ALPHA_FIRST_TWO.V;
       MEANING=SWI_meaning;
     "/>
  </item>
  <item>
    <ruleref uri="#CONTRACT_NUM_NUMERIC" tag="
       SWI_meaning = CONTRACT_NUM_NUMERIC.V;
       MEANING=SWI_meaning;
     "/>
  </item>
  <item>
    <ruleref uri="#MEMBER_NUM_MIXED_FIRST_FOUR" tag="
       SWI_meaning = MEMBER_NUM_MIXED_FIRST_FOUR.V;
       MEANING=SWI_meaning;
     "/>
  </item>
  </one-of>
  </rule>
  <rule id="CONTRACT_NUM_ALPHA_FIRST_TWO">
    <ruleref uri="#SUB_ALPHA_FIRST_TWO_CONTRACT" tag="V = SUB_ALPHA_FIRST_TWO_CONTRACT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
  </rule>
  <rule id="CONTRACT_NUM_NUMERIC">
    <ruleref uri="#DIGIT" tag="V = DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
  </rule>
  <rule id="MEMBER_NUM_MIXED_FIRST_FOUR">
    <ruleref uri="#SUB_ALPHANUMERIC_FIRST_FOUR" tag="V = SUB_ALPHANUMERIC_FIRST_FOUR.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>
    <ruleref uri="#DIGIT" tag="V += DIGIT.V"/>

  </rule>
  <rule id="DIGIT">
    <one-of>
      <item tag="V='0'">oh</item>
      <item tag="V='0'">zero</item>
      <item tag="V='1'">one</item>
      <item tag="V='2'">two</item>
      <item tag="V='3'">three</item>
      <item tag="V='4'">four</item>
      <item tag="V='5'">five</item>
      <item tag="V='6'">six</item>
      <item tag="V='7'">seven</item>
      <item tag="V='8'">eight</item>
      <item tag="V='9'">nine</item>
    </one-of>
  </rule>
  <rule id="SUB_ALPHANUMERIC_FIRST_FOUR">
    <one-of>
      <item tag="V='85D5'">eight five d five</item>
      <item tag="V='90D5'">nine oh d five</item>
      <item tag="V='90D5'">nine zero d five</item>
      <item tag="V='CA30'">c a three oh</item>
      <item tag="V='CA30'">c a three zero</item>
      <item tag="V='CA45'">c a four five</item>
      <item tag="V='CA56'">c a five six</item>
      <item tag="V='CA72'">c a seven two</item>
      <item tag="V='CA77'">c a seven seven</item>
      <item tag="V='CA80'">c a eight oh</item>
      <item tag="V='CA80'">c a eight zero</item>
      <item tag="V='CA88'">c a eight eight</item>
      <item tag="V='CA89'">c a eight nine</item>
      <item tag="V='CAAZ'">c a a z</item>
      <item tag="V='CACA'">c a c a</item>
      <item tag="V='CAD1'">c a d one</item>
      <item tag="V='CAD4'">c a d four</item>
      <item tag="V='CAD5'">c a d five</item>
      <item tag="V='CAVR'">c a v r</item>
      <item tag="V='CZD1'">c z d one</item>
      <item tag="V='CZD4'">c z d four</item>
      <item tag="V='CZD5'">c z d five</item>
      <item tag="V='DM06'">d m oh six</item>
      <item tag="V='DM06'">d m zero six</item>
      <item tag="V='DM07'">d m oh seven</item>
      <item tag="V='DM07'">d m zero seven</item>
      <item tag="V='DM85'">d m eight five</item>
      <item tag="V='DM90'">d m nine oh</item>
      <item tag="V='DM90'">d m nine zero</item>
      <item tag="V='DMD2'">d m d two</item>
      <item tag="V='DMD4'">d m d four</item>
      <item tag="V='DMD5'">d m d five</item>
      <item tag="V='HI05'">h i oh five</item>
      <item tag="V='HI05'">h i zero five</item>
      <item tag="V='HI10'">h i one oh</item>
      <item tag="V='HI10'">h i one zero</item>
      <item tag="V='HI12'">h i one two</item>
      <item tag="V='HI13'">h i one three</item>
      <item tag="V='HI20'">h i two zero</item>
      <item tag="V='HI20'">h i two oh</item>
      <item tag="V='HI25'">h i two five</item>
      <item tag="V='HI28'">h i two eight</item>
      <item tag="V='HIA2'">h i a two</item>
      <item tag="V='HIC1'">h i c one</item>
      <item tag="V='HID1'">h i d one</item>
      <item tag="V='HID4'">h i d four</item>
      <item tag="V='HID5'">h i d five</item>
      <item tag="V='HIEX'">h i e x</item>
      <item tag="V='HIHI'">h i h i</item>
      <item tag="V='HIVR'">h i v r</item>
      <item tag="V='PA91'">p a nine one</item>
      <item tag="V='PAPA'">p a p a</item>
    </one-of>
  </rule>

  <rule id="SUB_ALPHA_FIRST_TWO_CONTRACT">
    <one-of> 


      <item tag="V='AZ'">a z</item>  
      <item tag="V='CA'">c a</item>

      <item tag="V='DM'">d m</item>  
      <item tag="V='HI'">h i</item>

      <item tag="V='MW'">m w</item>  

      <item tag="V='NE'">n e</item>
      <item tag="V='PA'">p a</item>
      <item tag="V='SC'">s c</item>
    </one-of>
  </rule>

</grammar>

